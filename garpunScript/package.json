{
  "name": "harpoon",
  "version": "1.4.5",
  "type": "commonjs",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "description": "This is a app for adapting hikvision cameras to prepare and sending data to the API.",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=PROD node ./src/app",
    "watch": "tsc && concurrently \"tsc -w\" \"nodemon\"",
    "dev": "cross-env NODE_ENV=DEV npm run watch",
    "load": "node ./tests/load/load.test",
    "lint:status": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/chart.js": "^2.9.37",
    "axios": "^0.27.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.2.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-type": "^17.1.2",
    "moment": "^2.29.3",
    "node-schedule": "^2.1.0",
    "node-telegram-bot-api": "^0.58.0",
    "quickchart-js": "^3.0.1",
    "sequelize": "^6.21.2",
    "sqlite3": "^5.0.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.5.0",
    "gts": "^3.1.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.8",
    "nodemon": "^1.3.3",
    "sequelize-cli": "^6.4.1",
    "sequelize-mock": "^0.10.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
